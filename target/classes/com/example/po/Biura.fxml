<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.po.SceneController">
   <children>
      <BorderPane layoutX="-1.0" layoutY="-2.0" prefHeight="294.0" prefWidth="600.0">
         <left>
            <Button fx:id="DoRaportow" mnemonicParsing="false" onAction="#switchToRaporty" text="Sprawdź raporty" BorderPane.alignment="CENTER" />
         </left>
         <right>
            <Button fx:id="DoPrzelewow" mnemonicParsing="false" onAction="#switchToPrzelewy" text="Sprawdź przelewy" BorderPane.alignment="CENTER" />
         </right>
         <bottom>
            <Button fx:id="DoHolu" mnemonicParsing="false" onAction="#switchToHol" text="To głównego holu" BorderPane.alignment="CENTER" />
         </bottom>
         <top>
            <TextField text="Stoisz przed biurem. Co zamierzasz zrobić? " BorderPane.alignment="CENTER" />
         </top>
         <center>
            <Button fx:id="DoRachunków" mnemonicParsing="false" onAction="#switchToRachunki" text="Sprawdź rachunki" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</AnchorPane>
